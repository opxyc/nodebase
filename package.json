{
	"name": "nodebase",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"dev:docker": "docker-compose -f docker-compose.development.yaml up",
		"dev": "nodemon --legacy-watch",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"migrate": "sequelize-cli db:migrate",
		"prepare": "",
		"prod": "node -r tsconfig-paths/register dist/server.js",
		"release:dry": "semantic-release -d",
		"release:no-ci": "semantic-release --no-ci",
		"release": "semantic-release",
		"seed": "sequelize-cli db:seed:all",
		"start": "tsc && node -r tsconfig-paths/register dist/server.js",
		"test": "jest --forceExit --detectOpenHandles",
		"watch": "tsc -w"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/npm",
				{
					"npmPublish": false
				}
			],
			"@semantic-release/changelog",
			"@semantic-release/git"
		],
		"branches": [
			"main"
		]
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
